@model IEnumerable<ClinicaClient.Objects.Medicamento>
@{
    ViewBag.Title = "DetallesCita";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ClinicaClient.Objects.CitaDetallesObject citaDetalles =(ClinicaClient.Objects.CitaDetallesObject) Session["citaDetalles"];
    int id_cita = citaDetalles.id_cita;
    string paciente = citaDetalles.nombre;
    string nombreCompleto = citaDetalles.nombre+" "+citaDetalles.ape_pat+" "+citaDetalles.ape_mat;
    string correo = citaDetalles.usuario;
    string error = (string) Session["Error"];
    string fecha = DateTime.Now.ToString("dd/MM/yyyy");
    string fecha2 = DateTime.Now.ToString("ddMMyyyy");
}
<link href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" crossorigin="anonymous" />
<div class="">
    <h1>@error</h1>
    <input type="hidden" id="nombreCompleto" value="@nombreCompleto" />
    <input type="hidden" id="paciente" value="@paciente" />
    <ul class="body-tabs body-tabs-layout tabs-animated body-tabs-animated nav">
        <li class="nav-item">
            <a role="tab" class="nav-link active" id="tab-0" data-toggle="tab" href="#tab-content-0">
                <span>Detalles de la cita</span>
            </a>
        </li>
        <li class="nav-item">
            <a role="tab" class="nav-link" id="tab-1" data-toggle="tab" href="#tab-content-1">
                <span>Generar Receta</span>
            </a>
        </li>
    </ul><br /><br />
    <div class="tab-content">
        <div class="tab-pane tabs-animation fade show active" id="tab-content-0" role="tabpanel">
            <div class="row">
                <div class="col-md-12">
                    <h3 class="text-center" style="border: outset"><strong>Detalles Cita</strong></h3>
                    <table class="mb-0 table" style="font-size:16px; border:dotted">
                        <tr>
                            <td>Paciente</td>
                            <td>@nombreCompleto</td>
                        </tr>
                        <tr>
                            <td>Fecha de la cita</td>
                            <td>@citaDetalles.fecha</td>
                        </tr>
                        <tr>
                            <td>Hora de la cita</td>
                            <td>@citaDetalles.hora</td>
                        </tr>
                        <tr>
                            <td>Observación de la  cita</td>
                            <td>@citaDetalles.observacion</td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
        <div class="tab-pane tabs-animation fade" id="tab-content-1" role="tabpanel">
            <div class="row">
                <div class="col-md-12">
                    <h3 class="text-center text-black-50">Generar Receta</h3>
                    <div class="mb-3 card card-body">
                        @using (Html.BeginForm("DetallesCita", "Citas", FormMethod.Post, new { enctype = "multipart/form-data" }))

                        {
                            <input type="hidden" name="id_cita" value="@id_cita" />
                            <input type="hidden" name="correo" value="@correo" />


                            <h5 class="card-title">Formulario Receta</h5>
                            <div class="form-row">
                                <div class="col-md-12 mb-12">
                                    <label for="validationCustom01" class="text-black-50">Observaciones</label>
                                    <textarea class="form-control" id="observacion" name="observacion" placeholder="Obserciones" minlength="5" maxlength="150" title="debe ser una descripción de más de 5 caractéres y menos de 150" required></textarea>
                                </div>
                                <div class="col-md-12 mb-12">
                                    <br />
                                    <label for="validationCustom01" class="text-black-50">Instrucciones</label>
                                    <textarea class="form-control" id="indicaciones" name="indicaciones" placeholder="Indicaciones" minlength="5" maxlength="150" title="debe ser una descripción de más de 5 caractéres y menos de 150" required></textarea>
                                </div>
                                <div class="col-md-12 mb-12">
                                    <br />
                                    <label for="validationCustom01" class="text-black-50">Total</label>
                                    <input type="number" step="any" class="form-control" id="cobro" name="cobro" placeholder="$" min="0" title="No pueden ser número negativos" required />
                                </div>
                                <label for="validationCustom01" class="text-black-50">Medicamentos</label>
                                <div class="col-md-12 mb-12">
                                    <br />
                                    <table class="table table-hover text-center" id="dataTable_Medicamentos">
                                        <thead class="">
                                            <tr>
                                                <th class="text-center">
                                                    Nombre del medicamento
                                                </th>
                                                <th class="text-center">
                                                    Dosis
                                                </th>
                                                <th class="text-center">
                                                    Precio
                                                </th>
                                                <th>
                                                    Indicaciones
                                                </th>
                                                <th>Agregar</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in Model)
                                            {
                                                <tr>
                                                    <td>
                                                        @item.nombre
                                                    </td>
                                                    <td>
                                                        @item.dosis
                                                    </td>
                                                    <td>
                                                        @item.precio
                                                    </td>
                                                    <td>
                                                        @item.indicaciones
                                                    </td>
                                                    <td><input type="checkbox" id="ids_medicamentos" name="ids_medicamentos" value="@Json.Encode(item)" /></td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div><br /><br />
                            <button class="btn btn-block btn-primary" onclick="generarPDF()">Generar receta</button>
                            <br />
                            <style>
                                .file-upload {
                                    background-color: #ffffff;
                                    width: 600px;
                                    margin: 0 auto;
                                    padding: 20px;
                                }

                                .file-upload-btn {
                                    width: 100%;
                                    margin: 0;
                                    color: #fff;
                                    background: #1FB264;
                                    border: none;
                                    padding: 10px;
                                    border-radius: 4px;
                                    border-bottom: 4px solid #15824B;
                                    transition: all .2s ease;
                                    outline: none;
                                    text-transform: uppercase;
                                    font-weight: 700;
                                }

                                    .file-upload-btn:hover {
                                        background: #1AA059;
                                        color: #ffffff;
                                        transition: all .2s ease;
                                        cursor: pointer;
                                    }

                                    .file-upload-btn:active {
                                        border: 0;
                                        transition: all .2s ease;
                                    }

                                .file-upload-content {
                                    display: none;
                                    text-align: center;
                                }

                                .file-upload-input {
                                    position: absolute;
                                    margin: 0;
                                    padding: 0;
                                    width: 100%;
                                    height: 100%;
                                    outline: none;
                                    opacity: 0;
                                    cursor: pointer;
                                }

                                .image-upload-wrap {
                                    margin-top: 20px;
                                    border: 4px dashed #1FB264;
                                    position: relative;
                                }

                                    .image-dropping,
                                    .image-upload-wrap:hover {
                                        background-color: #1FB264;
                                        border: 4px dashed #ffffff;
                                    }

                                .image-title-wrap {
                                    padding: 0 15px 15px 15px;
                                    color: #222;
                                }

                                .drag-text {
                                    text-align: center;
                                }

                                    .drag-text h3 {
                                        font-weight: 100;
                                        text-transform: uppercase;
                                        color: #15824B;
                                        padding: 60px 0;
                                    }

                                .file-upload-image {
                                    max-height: 200px;
                                    max-width: 200px;
                                    margin: auto;
                                    padding: 20px;
                                }

                                .remove-image {
                                    width: 200px;
                                    margin: 0;
                                    color: #fff;
                                    background: #cd4535;
                                    border: none;
                                    padding: 10px;
                                    border-radius: 4px;
                                    border-bottom: 4px solid #b02818;
                                    transition: all .2s ease;
                                    outline: none;
                                    text-transform: uppercase;
                                    font-weight: 700;
                                }

                                    .remove-image:hover {
                                        background: #c13b2a;
                                        color: #ffffff;
                                        transition: all .2s ease;
                                        cursor: pointer;
                                    }

                                    .remove-image:active {
                                        border: 0;
                                        transition: all .2s ease;
                                    }
                            </style>

                            <div class="file-upload">


                                <div class="image-upload-wrap">
                                    <input class="file-upload-input" type='file' name="file" onchange="readURL(this);" accept="image/*" required />
                                    <div class="drag-text">
                                        <h3>Arrastra y suelta un archivo</h3>
                                    </div>
                                </div>
                                <div class="file-upload-content">
                                    <h2 class="text-center" style="border: outset"><strong>El archivo se cargo con éxito</strong></h2>
                                    <img width="100" height="100" src="https://upload.wikimedia.org/wikipedia/commons/thumb/c/c5/Blue_check_PD.svg/1024px-Blue_check_PD.svg.png" />
                                    <div class="image-title-wrap">
                                        <button type="button" onclick="removeUpload()" class="remove-image">Remover <span class="image-title">Uploaded Image</span></button>
                                    </div>
                                </div>
                            </div>
                            <br /><br />
                            <button type="submit" class="file-upload-btn file-upload-content">Subir archivo</button>

                        }
                    </div>

                </div>

            </div>
        </div>
    </div>
</div>


    @Scripts.Render("~/bundles/jquery")
    <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js" crossorigin="anonymous"></script>
    
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.5.3/jspdf.min.js"></script>
    <script src='https://unpkg.com/jspdf-autotable@2.3.2'></script>

    <script>
        $('#dataTable_Medicamentos').DataTable({
            "destroy": false,
            pageLength: 7,
            "language": {
                "url": "https://cdn.datatables.net/plug-ins/1.10.15/i18n/Spanish.json"
            },
            lengthMenu: [[5, 10, 15], [5, 10, 15]]
        });


        function readURL(input) {
            if (input.files && input.files[0]) {

                var reader = new FileReader();

                reader.onload = function (e) {
                    $('.image-upload-wrap').hide();

                    $('.file-upload-image').attr('src', e.target.result);
                    $('.file-upload-content').show();

                    $('.image-title').html(input.files[0].name);
                };

                reader.readAsDataURL(input.files[0]);

            } else {
                removeUpload();
            }
        }

        function removeUpload() {
            $('.file-upload-input').replaceWith($('.file-upload-input').clone());
            $('.file-upload-content').hide();
            $('.image-upload-wrap').show();
        }
        $('.image-upload-wrap').bind('dragover', function () {
            $('.image-upload-wrap').addClass('image-dropping');
        });
        $('.image-upload-wrap').bind('dragleave', function () {
            $('.image-upload-wrap').removeClass('image-dropping');
        });


        function generarPDF() {
            var observacion = document.getElementById('observacion').value;
            var indicaciones = document.getElementById('indicaciones').value;
            var cobro = "$" + document.getElementById('cobro').value;
            var paciente = document.getElementById('paciente').value;
            var nombre = document.getElementById('nombreCompleto').value;
            console.log(observacion.lngth)
            if ((observacion.length > 5) && (indicaciones.length > 5) && (cobro.length > 1)) {



                console.log("generando pdf ...");

                var logo = 'data:image/png;base64,';
                var doc = new jsPDF();
                //Cabecera
                doc.addImage(logo, 'JPEG', 20, 10, 35, 35);
                doc.setFontSize(20);
                doc.setTextColor(80, 0, 255);
                doc.text(60, 30, 'Dr. Erick Mitchell Echeverría Celaya');
                doc.setTextColor(0, 0, 0);
                doc.setFontSize(12);
                doc.setFont('courier');
                doc.setFontType('normal');
                doc.text(60, 40, 'Clinica "Mitchell" - Cédula profesional. 901014585');
                //Datos paciente
                doc.setFont('helvetica');
                doc.setFontType('bold');
                doc.setFontSize(14);
                doc.text(20, 65, 'Nombre del paciente: ');
                doc.setFont('helvetica');
                doc.setFontType('normal');
                doc.setFontSize(14);
                doc.text(76, 65, nombre);

                doc.setFont('helvetica');
                doc.setFontType('bold');
                doc.setFontSize(14);
                doc.text(20, 75, 'Fecha: ');
                doc.setFont('helvetica');
                doc.setFontType('normal');
                doc.setFontSize(14);
                doc.text(39, 75, '@fecha');

                doc.setFont('helvetica');
                doc.setFontType('bold');
                doc.setFontSize(14);
                doc.text(165, 75, 'Total: ');
                doc.setFont('helvetica');
                doc.setFontType('normal');
                doc.setFontSize(14);
                doc.text(180, 75, cobro);

                doc.setFont('helvetica');
                doc.setFontType('bold');
                doc.setFontSize(14);
                doc.text(20, 95, 'Observación: ');

                var lines = doc.setFont('Times', 'Roman')
                    .setFontSize(14)
                    .splitTextToSize(observacion, 142);
                doc.text(55, 95, lines);

                doc.setFont('helvetica');
                doc.setFontType('bold');
                doc.setFontSize(14);
                doc.text(20, 120, 'Indicaciones: ');

                var lines = doc.setFont('Times', 'Roman')
                    .setFontSize(14)
                    .splitTextToSize(indicaciones, 142);
                doc.text(55, 120, lines);

                //Tabla
                var columns = ["Medicamento", "Dosis", "Indicaciones"];
                var medicamentos = document.getElementsByName('ids_medicamentos');
                var aux;
                var data = [];
                medicamentos.forEach(x => {
                    if (x.checked) {
                        aux = JSON.parse(x.value);
                        data.push([aux.nombre, aux.dosis, aux.indicaciones]);
                    }
                });

                doc.autoTable(columns, data, {
                    startY: false,
                    theme: 'grid', //grid
                    columnWidth: 'wrap',
                    showHeader: 'everyPage',
                    columnStyles: {
                        0: { columnWidth: '100' },
                        1: { columnWidth: '100' },
                        2: { columnWidth: '100' }
                    },
                    headerStyles: {
                    },
                    styles: { overflow: 'linebreak', columnWidth: '100', font: 'arial', textColor: '#000', fontSize: 11, cellPadding: 4, overflowColumns: 'linebreak', halign: 'left' },
                    margin: { top: 140 }
                });
                var fondo ='data:image/png;base64,'
                doc.addImage(fondo, 'PNG', 45, 165, 120, 80);

                //footer
                doc.setFont('helvetica');
                doc.setFontType('normal');
                doc.setFontSize(11);
                doc.text(20, 288, 'Av. Bustamante No. 45  Zimatlán de Álvares  Tel: 654-51-23-01  Cel: 951014596  erisso@gmail.com');

                doc.save(paciente + '.pdf');

            }

        }

    </script>
